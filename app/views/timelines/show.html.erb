<h1>Timelines#show</h1>
<p>Find me in app/views/timelines/show.html.erb</p>
<%= render "timelines/profile_header", user: @visited_user %>
<% if on_self? %>
  <%= render("posts/form")  %>
<% end %>
<!--%= render "timelines/all" %-->
<% if can? current_user, :see_timeline, @visited_user %>
  <%= render "timelines/all" %>
<% else %>
  <p style="text-align: center">These posts are protected</p>
<% end %>
